#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int count(int n)
{
     int *answer=new int[n+1];
    answer[0]=0;
    answer[1]=1;  
    answer[2]=2;
    for(int i=3;i<=n;i++)
    {
        int min=INT_MAX;
        int a;
       for(int j=1;j<=sqrt(i);j++)
       {
       //a is like n-1^2 or n-2^2
           a=i-j*j;
           if(answer[a]<min)
           {
               min=answer[a];
           }
       }
       answer[i]=1+min;    
    }
   return answer[n];
}
int main() 
{
    int n;
    cin>>n;
    cout<<count(n);  
}
